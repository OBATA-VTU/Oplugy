{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/paystack",
      "destination": "/api/paystack-proxy.ts"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}