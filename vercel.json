
{
  "version": 2,
  "cleanUrls": true,
  "framework": "create-react-app",
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@3.2.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
